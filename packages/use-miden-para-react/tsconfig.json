{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "moduleResolution": "Node",
    "baseUrl": ".",
    "paths": {
      "@miden-sdk/miden-para": ["../../src/index.ts"],
      "@getpara/web-sdk": [
        "./node_modules/@getpara/web-sdk",
        "../../node_modules/@getpara/web-sdk"
      ]
    },
    "jsx": "react-jsx",
    "lib": ["DOM", "ES2019"],
    "strict": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "types": ["react"]
  },
  "include": ["src/**/*"]
}
