  "peerDependencies": {
    "@miden-sdk/miden-sdk": "^0.13.1",
    "@getpara/react-sdk-lite": "^2.11.0",
    "@getpara/web-sdk": "2.11.0",
    "@miden-sdk/miden-para": "^0.13.0",
    "@miden-sdk/react": "^0.13.1",
    "@tanstack/react-query": "^5.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@miden-sdk/miden-sdk": "^0.13.1",
    "@getpara/react-sdk-lite": "^2.11.0",
    "@getpara/web-sdk": "2.11.0",
    "@miden-sdk/react": "^0.13.1",
    "@tanstack/react-query": "^5.90.12",
    "@types/react": "^19.2.5",
    "tsup": "^8.3.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
